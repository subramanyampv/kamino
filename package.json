{
  "name": "generator-csharp-nuget-lib",
  "version": "2.2.0",
  "description": "Yeoman generator for a C# library that publishes a NuGet package. Includes a unit test project.",
  "main": "app/index.js",
  "repository": "ngeor/generator-csharp-nuget-lib",
  "scripts": {
    "pretest": "eslint .",
    "test": "nyc mocha",
    "coveralls": "nyc mocha && nyc report --reporter=text-lcov | coveralls",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "author": "Nikolaos Georgiou",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.6.2",
    "uuid": "^3.3.3",
    "yeoman-generator": "^4.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "app/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "all": true
  }
}
