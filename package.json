{
  "name": "generator-csharp-nuget-lib",
  "version": "1.1.1",
  "description": "Yeoman generator for a C# library that publishes a NuGet package. Includes a unit test project.",
  "main": "app/index.js",
  "repository": "ngeor/generator-csharp-nuget-lib",
  "scripts": {
    "test": "gulp",
    "coverage": "nyc mocha",
    "coveralls": "nyc mocha && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "author": "Nikolaos Georgiou",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.6.1",
    "uuid": "^3.2.1",
    "yeoman-generator": "^2.0.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-ngeor": "^0.2.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.2",
    "gulp-mocha": "^5.0.0",
    "gulp-stripbom": "^1.0.4",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.2"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "app/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "all": true
  }
}
